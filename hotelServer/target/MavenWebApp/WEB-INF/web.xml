<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>roomPage</servlet-name>
    <servlet-class>com.demo.RoomServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CustomerPage</servlet-name>
    <servlet-class>com.demo.CustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BookingPage</servlet-name>
    <servlet-class>com.demo.BookingServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>roomPage</servlet-name>
    <url-pattern>/rooms/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CustomerPage</servlet-name>
    <url-pattern>/customers/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BookingPage</servlet-name>
    <url-pattern>/bookings/*</url-pattern>
  </servlet-mapping>
</web-app>
